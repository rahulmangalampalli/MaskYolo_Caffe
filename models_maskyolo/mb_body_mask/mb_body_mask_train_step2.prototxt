    num_output: 256
    bias_term: false
    kernel_size: 1
    weight_filler { type: "gaussian" std: 0.01 }
  }
}

layer {
  name: "conv5/3/ds/relu"
  type: "ReLU"
  bottom: "conv5/3/ds"
  top: "conv5/3/ds"
}

